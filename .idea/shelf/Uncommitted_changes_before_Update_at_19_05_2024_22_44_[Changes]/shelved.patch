+                this.lista[pos] = o;
+                añadido = true;
+                //System.out.println("Elemento añadido");
+                break;
+            }
+            pos++;
+        }
+        if (!añadido) {
+            System.out.println("Lista Llena");
+        }
+        return pos;
+    }
+
+    public void add(String dato) {
